{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Launch via Babel",
          "program": "${workspaceRoot}/index.js",
          "sourceMaps": true,
          "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
          "cwd": "${workspaceRoot}",
          "protocol": "inspector",
          "outputCapture": "console"
      }
  ]
}

